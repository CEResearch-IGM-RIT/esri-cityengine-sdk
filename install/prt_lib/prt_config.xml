<?xml version="1.0" encoding="UTF-8"?>
<prt>
  <!--  valid types: geometry, texture, cgb, initialshape -->
        
  <adaptor id="com.esri.prt.adaptors.zipfile">
    <library>com.esri.prt.adaptors.fs</library>
    <search>prt_lib</search>
 	<use_decoder contentType="geometry">com.esri.prt.codecs.OBJDecoder</use_decoder>
    <use_decoder contentType="material">com.esri.prt.codecs.MTLDecoder</use_decoder>
    <use_decoder contentType="texture">com.esri.prt.codecs.JPGDecoder</use_decoder>
    <use_decoder contentType="texture">com.esri.prt.codecs.TIFDecoder</use_decoder>
    <use_decoder contentType="texture">com.esri.prt.codecs.PNGDecoder</use_decoder>
  </adaptor>

  <adaptor id="com.esri.prt.adaptors.filesystem">
    <library>com.esri.prt.adaptors.fs</library>
    <search>prt_lib</search>
    <use_decoder contentType="geometry">com.esri.prt.codecs.OBJDecoder</use_decoder>
    <use_decoder contentType="material">com.esri.prt.codecs.MTLDecoder</use_decoder>
    <use_decoder contentType="texture">com.esri.prt.codecs.JPGDecoder</use_decoder>
    <use_decoder contentType="texture">com.esri.prt.codecs.TIFDecoder</use_decoder>
    <use_decoder contentType="texture">com.esri.prt.codecs.PNGDecoder</use_decoder>
  </adaptor>

  <decoder id="com.esri.prt.codecs.OBJDecoder" contentType="geometry">
    <library>com.esri.prt.codecs</library>
    <search>prt_lib</search>
  </decoder>

  <decoder id="com.esri.prt.codecs.MTLDecoder" contentType="material">
    <library>com.esri.prt.codecs</library>
    <search>prt_lib</search>
  </decoder>

  <decoder id="com.esri.prt.codecs.JPGDecoder" contentType="texture">
    <library>com.esri.prt.codecs</library>
    <search>prt_lib</search>
  </decoder>

  <decoder id="com.esri.prt.codecs.TIFDecoder" contentType="texture">
    <library>com.esri.prt.codecs</library>
    <search>prt_lib</search>
  </decoder>

  <decoder id="com.esri.prt.codecs.PNGDecoder" contentType="texture">
    <library>com.esri.prt.codecs</library>
    <search>prt_lib</search>
  </decoder>
  
  <encoder id="com.esri.prt.codecs.MayaEncoder" contentType="geometry">
    <library>com.esri.prt.codecs.maya</library>
    <search>prt_lib</search>
  </encoder>
  
</prt>